{
  "dataStore": {
    "dbPath": "data",
    "backupPath": "backup",
    "storeMemoryLimit": "5120MB",
    "storeWarmStart": true,
    "concurrency": 8,
    "logWriteMode": 1,
    "persistencyMode": "NORMAL",
    "affinityGroupSize": 4
  },
  "checkpoint": {
    "checkpointInterval": "1200s",
    "checkpointMemoryLimit": "1024MB",
    "useParallelMode": false
  },
  "cluster": {
    "clusterName": "dockerGridDB",
    "notificationMember": [
      {
        "host": "127.0.0.1",
        "port": 10001
      }
    ],
    "replicationNum": 1,
    "servicePort": 10010
  },
  "sync": {
    "servicePort": 10020
  },
  "system": {
    "servicePort": 10040,
    "eventLogPath": "log"
  },
  "transaction": {
    "serviceAddress": "127.0.0.1",
    "servicePort": 10001,
    "connectionLimit": 5000
  },
  "sql": {
    "serviceAddress": "127.0.0.1",
    "servicePort": 20001
  },
  "trace": {
    "default": "LEVEL_ERROR",
    "dataStore": "LEVEL_ERROR",
